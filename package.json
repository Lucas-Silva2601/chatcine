{
  "name": "chatcine",
  "version": "2.0.0",
  "description": "ChatCine - Aplicação Full Stack com React + Flask",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && python run.py",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview",
    "install:all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "install:frontend": "cd frontend && npm install",
    "init-db": "cd backend && python init_db.py",
    "test:backend": "cd backend && pytest",
    "test:frontend": "cd frontend && npm run test",
    "lint:backend": "cd backend && flake8 .",
    "lint:frontend": "cd frontend && npm run lint"
  },
  "keywords": [
    "react",
    "flask",
    "chat",
    "movies",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
