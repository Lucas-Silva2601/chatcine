{
  "name": "chatcine",
  "version": "1.0.0",
  "description": "ChatCine - Aplicação Flask para chat sobre filmes com IA",
  "scripts": {
    "dev": "python run.py",
    "start": "python run.py",
    "test": "pytest",
    "test:cov": "pytest --cov=chatcine --cov-report=html",
    "test:watch": "pytest-watch",
    "lint": "flake8 chatcine && black --check chatcine",
    "lint:fix": "black chatcine",
    "type-check": "mypy chatcine",
    "init-db": "python init_db.py",
    "migrate": "flask db migrate",
    "upgrade": "flask db upgrade",
    "setup": "python -m venv venv && pip install -r requirements.txt",
    "install": "pip install -r requirements.txt"
  },
  "keywords": [
    "flask",
    "python",
    "chat",
    "movies",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0",
    "python": ">=3.9.0"
  }
}

